		function showTab1_1(){
			var tab1_1=document.getElementById("tab1_1");

			if(tab1_1.style.display=='none'){
				tab1_1.style.display='block';
			}else{
				tab1_1.style.display='none';
			}
		}
		function showTab2(){
			var tab2=document.getElementById("tab2");
			
			if(tab2.style.display=='none'){
